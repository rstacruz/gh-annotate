{
  "name": "gh-annotate",
  "version": "0.3.0",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run build-man",
    "build-man": "if which ronn; then ronn man/*.md --html --roff --style=toc,80c --organization=\"@rstacruz\"; fi",
    "test": "mocha"
  },
  "bin": {
    "gh-annotate": "./bin/gh-annotate"
  },
  "author": "Rico Sta. Cruz <hi@ricostacruz.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^2.2.0",
    "mocha": "^2.2.1",
    "mocha-clean": "^0.4.0"
  },
  "directories": {
    "man": "man",
    "test": "test"
  },
  "dependencies": {
    "github-slug": "0.0.2",
    "read-input": "^0.3.1",
    "yargs": "^3.7.0"
  },
  "description": "Adds GitHub links to Markdown files. Perfect for maintaining changelog files. See: [gh-annotate(1)].",
  "repository": {
    "type": "git",
    "url": "https://github.com/rstacruz/gh-annotate.git"
  },
  "bugs": {
    "url": "https://github.com/rstacruz/gh-annotate/issues"
  },
  "homepage": "https://github.com/rstacruz/gh-annotate"
